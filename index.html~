// variabili di login //

//var socket = new WebSocket("ws://localhost:30000/index.html");

var socket = new WebSocket("ws://hearts-lellolandi.rhcloud.com/:8000");
var userreg = "";
var name = "";
var passwd = "";
var onlinestring = "";
var tableids = [];
var iCreate = false;
var intable = 0;
var idtable = 0;

// variabili di gioco //

var fase = "pass";
var pass = 0;
var mano = [];
var passate = [];
var ricevute = [];
var tavolo = [];
var tocca = false;
var toccaux = false;
var broken = false;
var primo = -1;
var giocate = 0;
